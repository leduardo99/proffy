{
  "name": "proffy",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn types:generate && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@unform/core": "^2.1.6",
    "@unform/web": "^2.1.6",
    "apollo": "^2.33.2",
    "graphql": "^15.5.0",
    "next": "10.2.0",
    "next-auth": "^3.23.1",
    "next-pwa": "^5.2.21",
    "nextjs-progressbar": "^0.0.9",
    "polished": "^4.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-select": "^4.3.1",
    "react-toastify": "^7.0.4",
    "styled-breakpoints": "^10.0.0",
    "styled-components": "5.3.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/react": "6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.3",
    "@types/react-input-mask": "^3.0.0",
    "@types/react-select": "^4.0.15",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^11.0.0",
    "plop": "^2.7.4",
    "prettier": "^2.3.0",
    "storybook-addon-next-router": "^2.0.4",
    "typescript": "^4.2.4"
  }
}
